  "homepage": "https://github.com/AmplicaLabs/dsnp-gateway-prototype#readme",
  "dependencies": {
    "@amplica-labs/siwf": "file:vendor/amplica-labs-siwf-0.1.0.tgz",
    "@dsnp/activity-content": "^1.1.0",
    "@dsnp/frequency-schemas": "^1.1.0",
    "@dsnp/parquetjs": "^1.6.2",
    "@frequency-chain/api-augment": "^1.11.0",
    "@multiformats/blake2": "^2.0.2",
    "@polkadot/api": "^10.9.1",
    "@polkadot/util": "^12.6.2",
    "avro-js": "^1.11.3",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multiformats": "^13.1.0",
    "openapi-backend": "^5.10.6",
    "swagger-ui-express": "^5.0.0"
