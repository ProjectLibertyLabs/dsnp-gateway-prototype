    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "openapi-client-axios-typegen": "^7.6.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.3.1",
    "wait-on": "^7.2.0"
  }
